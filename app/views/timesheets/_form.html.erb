<%= simple_form_for([@user, @timesheet]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :teacher %>
    <%= f.input :study %>
    <%= f.input :conversation %>
    <%= f.input :day, collection: ["Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday"] %>
 
    
  </div>

  <div class="form-actions">
    <%= f.button :submit%>
  </div>
<% end %>
