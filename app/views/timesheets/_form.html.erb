<%= simple_form_for([@user, @timesheet]) do |f| %>
  <%= render 'error_messages' %>


  <div class="form-inputs">
    <%= f.input :teacher %>
    <%= f.input :study %>
    <%= f.input :conversation %>
    <%= f.input :day, :required => true, :input_html => { data: {behaviour: "datepicker"}}, :as => :string %>



  </div>

  <div class="form-actions">
    <%= f.button :submit%>
  </div>
<% end %>
